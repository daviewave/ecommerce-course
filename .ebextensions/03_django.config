container_commands:
  01_makemigrations:
    command: "source ~/dav_repos/ecommerce-django/eb-venv/bin/activate && python3 manage.py makemigrations --noinput"
    leader_only: true
  02_migrate:
    command: "source ~/dav_repos/ecommerce-django/eb-venv/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true
  03_superuser:
    command: "source ~/dav_repos/ecommerce-django/eb-venv/bin/activate && python3 manage.py create_super_user"
    leader_only: true